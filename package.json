    "@types/autoprefixer": "^10.2.4",
    "@types/jest": "^29.5.6",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "puppeteer": "^21.9.0",
    "tailwindcss": "^3.4.16"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/sass": "^3.0.12",
